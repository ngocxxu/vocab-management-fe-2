# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3001

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NESTJS_API_URL=http://localhost:3000

# Production: set if auth cookies use a shared domain (e.g. .yourdomain.com) so signout can clear them
COOKIE_DOMAIN=.yourdomain.com

# WebSocket Configuration
# Set this to your WebSocket server URL (e.g., your backend server)
# Example: ws://localhost:3000 or wss://your-domain.com
NEXT_PUBLIC_SOCKET_URL=ws://localhost:3002

# Authentication (if using external auth providers)
# Add your auth provider keys here

# Analytics (optional)
NEXT_PUBLIC_POSTHOG_KEY=your_posthog_key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Monitoring (optional)
NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN=your_better_stack_token
NEXT_PUBLIC_BETTER_STACK_INGESTING_HOST=https://in.logs.betterstack.com

# Security (optional)
ARCJET_KEY=ajkey_your_arcjet_key

# Environment
NODE_ENV=development

